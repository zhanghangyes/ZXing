<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextAppear.Theme.f_Size18" parent="android:TextAppearance.Theme">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/f</item>
    </style>

    <style name="TextAppear.Theme.cb9_Size14" parent="android:TextAppearance.Theme">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/cb9</item>
    </style>

    <style name="TextAppear.Theme.f_Size14" parent="android:TextAppearance.Theme">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/f</item>
    </style>

    <style name="TextAppear.Theme.f_Size15" parent="android:TextAppearance.Theme">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/f</item>
    </style>

</resources>
